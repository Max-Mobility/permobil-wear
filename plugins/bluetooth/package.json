{
  "name": "nativescript-bluetooth",
  "version": "1.3.0",
  "description": "Connect to and interact with Bluetooth LE peripherals",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.3.0",
      "ios": "2.3.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Processing"
    }
  },
  "snapshot": {
    "android": {
      "tns-java-classes": {
        "modules": [
          "nativescript-bluetooth/android/TNS_AdvertiseCallback",
          "nativescript-bluetooth/android/TNS_BluetoothGattCallback",
          "nativescript-bluetooth/android/TNS_BluetoothGattServerCallback",
          "nativescript-bluetooth/android/TNS_BroadcastReceiver",
          "nativescript-bluetooth/android/TNS_LeScanCallback",
          "nativescript-bluetooth/android/TNS_ScanCallback"
        ]
      }
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eddyverbruggen/nativescript-bluetooth.git"
  },
  "keywords": [
    "ecosystem:NativeScript",
    "NativeScript",
    "BLE",
    "Bluetooth",
    "Bluetooth LE",
    "Bluetooth Smart",
    "Bluetooth Low Energy"
  ],
  "author": {
    "name": "Eddy Verbruggen",
    "email": "eddyverbruggen@gmail.com"
  },
  "contributors": [
    {
      "name": "Brad Martin",
      "email": "bmartin@nstudio.io",
      "url": "https://github.com/bradmartin"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eddyverbruggen/nativescript-bluetooth/issues"
  },
  "homepage": "https://github.com/eddyverbruggen/nativescript-bluetooth",
  "devDependencies": {
    "tns-core-modules": "^5.2.1",
    "tns-platform-declarations": "^5.2.1",
    "typescript": "~3.1.6"
  }
}
