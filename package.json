{
  "nativescript": {
    "id": "com.permobil.watch",
    "tns-android": {
      "version": "5.0.0-rc-2018-10-26-162351-01"
    }
  },
  "main": "app.js",
  "description": "NativeScript Application",
  "license": "SEE LICENSE IN <your-license-filename>",
  "repository": "<fill-your-repository-here>",
  "scripts": {
    "precommit": "lint-staged",
    "start.android": "tns run android",
    "start.ios": "tns run ios",
    "build-bluetooth": "cd nativescript-bluetooth && npm i && tsc && rm -rf *.tgz && npm pack",
    "build-accelerometer": "cd nativescript-accelerometer && npm i && tsc && rm -rf *.tgz && npm pack",
    "build-packages": "npm run build-bluetooth && npm run build-accelerometer",
    "nuke": "rm -rf platforms hooks node_modules package-lock.json",
    "nuki": "npm run nuke && npm run setup",
    "setup": "npm i && npm run build-packages"
  },
  "dependencies": {
    "buffer": "^5.2.0",
    "nativescript-accelerometer-advanced": "file:./nativescript-accelerometer/nativescript-accelerometer-advanced-3.0.0.tgz",
    "nativescript-bluetooth": "file:./nativescript-bluetooth/nativescript-bluetooth-1.3.0.tgz",
    "nativescript-carousel": "^4.0.2",
    "nativescript-geolocation": "^4.3.1",
    "nativescript-gif": "^4.0.1",
    "nativescript-lottie": "^2.1.1",
    "nativescript-permissions": "^1.2.3",
    "nativescript-theme-core": "~1.0.4",
    "nativescript-toasty": "1.2.0",
    "tns-core-modules": "^5.0.0-rc-2018-10-30-144157-01"
  },
  "devDependencies": {
    "husky": "^1.1.2",
    "lint-staged": "^8.0.3",
    "nativescript-dev-typescript": "~0.7.4",
    "nativescript-dev-webpack": "^0.17.0-2018-10-25-144355-01",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "tns-platform-declarations": "^4.2.1",
    "tslint": "~5.11.0",
    "typescript": "~2.8.2"
  },
  "lint-staged": {
    "*.{ts, js, json, css, scss, md}": [
      "prettier --write",
      "git add"
    ]
  }
}