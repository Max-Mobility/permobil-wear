{
  "nativescript": {
    "id": "com.permobil.watch",
    "tns-android": {
      "version": "5.2.1"
    }
  },
  "main": "app.js",
  "description": "NativeScript Application",
  "license": "SEE LICENSE IN <your-license-filename>",
  "repository": "https://github.com/Max-Mobility/permobil-wear",
  "scripts": {
    "// Brad Section": "*****************************************************************************************************",
    "brad.debug.wifi": "adb tcpip 5555 && adb connect 192.168.1.65",
    "// end Brad Section": "************************************************************************************************************",
    "android.release": "",
    "clean.accelerometer": "cd nativescript-accelerometer && rimraf node_modules",
    "clean.bluetooth": "cd nativescript-bluetooth && rimraf node_modules",
    "clean": "rimraf platforms hooks node_modules package-lock.json",
    "nuke": "npm run clean.accelerometer && npm run clean.bluetooth && npm run clean",
    "nuki": "npm run nuke && npm run setup",
    "setup": "npm i -g rimraf && npm run nuke && npm i && npm run setup.accelerometer && npm run setup.bluetooth && npm i",
    "setup.accelerometer": "cd nativescript-accelerometer && npm i",
    "setup.bluetooth": "cd nativescript-bluetooth && npm i",
    "start.android": "tns run android --bundle",
    "start.android.hmr": "tns run android --bundle --hmr",
    "start.android.uglify": "tns run android --bundle --env.uglify",
    "start.android.snapshot": "tns run android --bundle --env.uglify --env.snapshot",
    "tslint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "buffer": "~5.2.0",
    "injection-js": "~2.2.1",
    "kinvey-nativescript-sdk": "~3.12.4",
    "nativescript-accelerometer-advanced": "file:./nativescript-accelerometer",
    "nativescript-bluetooth": "file:./nativescript-bluetooth",
    "nativescript-geolocation": "~4.5.0",
    "nativescript-gif": "~4.0.2",
    "nativescript-lottie": "~3.0.2",
    "nativescript-permissions": "~1.2.3",
    "nativescript-sentry": "~1.8.0",
    "nativescript-theme-core": "~1.0.4",
    "nativescript-toasty": "~1.2.0",
    "nativescript-ui-chart": "~3.11.1",
    "nativescript-wear-os": "~0.1.41",
    "reflect-metadata": "~0.1.13",
    "smart-eval-kinvey": "git+https://github.com/autochair/SmartEvalKinvey.git#master",
    "tns-core-modules": "~5.2.2"
  },
  "devDependencies": {
    "husky": "~1.3.1",
    "lint-staged": "~8.1.5",
    "mkdirp": "~0.5.1",
    "awesome-typescript-loader": "~5.2.1",
    "nativescript-dev-typescript": "~0.8.0",
    "nativescript-dev-webpack": "~0.20.2",
    "nativescript-hook": "~0.2.5",
    "prettier": "~1.16.4",
    "prompt": "~1.0.0",
    "rimraf": "~2.6.3",
    "tns-platform-declarations": "~5.2.2",
    "tslint": "~5.13.1",
    "typescript": "~3.1.6"
  },
  "lint-staged": {
    "*.{ts, js, json, css, scss, md}": [
      "tslint",
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": true
  }
}
